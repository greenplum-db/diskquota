-- tables
SELECT
    relname,
    typeid_to_name(ARRAY[c.reltype]::oid[]) as reltype,
    typeid_to_name(ARRAY[c.reloftype]::oid[]) as reloftype
FROM
    pg_class c,
    pg_namespace n
WHERE
    c.relnamespace = n.oid
    AND n.nspname = 'diskquota'
    and c.relkind != 'v'
ORDER BY
    relname;
